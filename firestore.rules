rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are completely insecure and allow public access
    // to your entire database. This is intended for development and
    // debugging ONLY.
    // You MUST secure your database before deploying to production.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
